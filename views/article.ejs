     <!--title,author,dateCreated,description-->
        <h1>Edit Document</h1>
    <form action="/article/create" method="POST" enctype="multipart/form-data">
        <label for="title">Title</label>
        <input type="text" name="title" id="title"  required> <br>
    
        <label for="author">Author</label>           
        <input type="text" name="author" id="author" required><br>
    
        <label for="dateCreated">Date</label>
        <input type="date" name="dateCreated" id="dateCreated" required><br>
    
        <label for="description">Description</label>
        <input type="text" name="description" id="description"   required><br>
        
        <label for="subDescription">Sub Description</label>
        <input type="text" name="subDescription" id="subDescription">

        <label for="file">File:</label>
        <input type="file" name="file" >
      

        <button type="submit">Submit</button>
    </form>
    
